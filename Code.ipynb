    "import os\n",
    "import pdf2image\n",
    "import pandas as pd\n",
    "import re\n",
    "import pytesseract\n",
    "from pdf2image import convert_from_path\n",
    "\n",
    "# Define file paths and necessary configurations\n",
    "pdf_file_path = #add file path,
    "poppler_path = #add poppler path,
    "tesseract_cmd_path = #r\add tesserct path,
    "\n",
    "# Convert PDF to images\n",
    "images = convert_from_path(pdf_file_path, poppler_path=poppler_path)\n",
    "\n",
    "# Create a folder to save the images if it doesn't exist\n",
    "output_folder_name = os.path.splitext(os.path.basename(pdf_file_path))[0]\n",
    "output_folder_path = os.path.join(os.getcwd(), output_folder_name)\n",
    "if not os.path.exists(output_folder_path):\n",
    "    os.mkdir(output_folder_path)\n",
    "print(\"Folder created successfully\")\n",
    "\n",
    "# Save each image as PNG\n",
    "for i, image in enumerate(images):\n",
    "    file_name = f\"{output_folder_path}\\\\Admit_Card_{i + 1}.png\"\n",
    "    print(file_name)\n",
    "    image.save(file_name, 'PNG')\n",
    "\n",
    "# Set Tesseract command path\n",
    "pytesseract.pytesseract.tesseract_cmd = tesseract_cmd_path\n",
    "\n",
    "# Initialize lists to store extracted data\n",
    "student_names = []\n",
    "student_PRNs = []\n",
    "student_seat_nos = []\n",
    "\n",
    "# Define regex patterns for extracting information\n",
    "student_name_pattern = re.compile(\"[A-Z.]{1,10}[ ][A-Z.]{1,10}[ ][A-Z.]{1,10}\")\n",
    "seat_no_pattern = re.compile(r\"\\b\\d{7}\\b\")\n",
    "prn_no_pattern = re.compile(r\"\\b\\d{16}\\b\")\n",
    "\n",
    "# Extract text and relevant information from each image\n",
    "for file in os.listdir(output_folder_path):\n",
    "    if file.startswith('Admit_Card_'):\n",
    "        image_path = os.path.join(output_folder_path, file)\n",
    "        text = pytesseract.image_to_string(image_path)\n",
    "\n",
    "        # Extract and append student name\n",
    "        student_name_match = student_name_pattern.search(text)\n",
    "        student_names.append(student_name_match.group() if student_name_match else '')\n",
    "\n",
    "        # Extract and append seat number\n",
    "        seat_no_match = seat_no_pattern.search(text)\n",
    "        student_seat_nos.append(seat_no_match.group() if seat_no_match else '')\n",
    "\n",
    "        # Extract and append PRN number\n",
    "        prn_no_match = prn_no_pattern.search(text)\n",
    "        student_PRNs.append(prn_no_match.group() if prn_no_match else '')\n",
    "\n",
    "# Create a DataFrame and save to CSV\n",
    "data = {'Name': student_names, 'PRN NO': student_PRNs, 'Seat NO': student_seat_nos}\n",
    "df = pd.DataFrame(data)\n",
    "df.to_csv('Student_AdmitCards.csv', index=False)\n",
    "print(\"Data extraction complete. CSV file created successfully.\")"
   ]
